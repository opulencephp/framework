{
    "name": "opulence/framework",
    "description": "The Opulence main framework library",
    "keywords": [
        "PHP",
        "application",
        "framework"
    ],
    "authors": [
        {
            "name": "David Young",
            "email": "dave@opulencephp.com",
            "role": "Architect"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Opulence\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opulence\\Framework\\Tests\\": "tests"
        }
    },
    "require": {
        "aphiria/console": "1.0.*@dev",
        "aphiria/dependency-injection": "1.0.*@dev",
        "aphiria/middleware": "1.0.*@dev",
        "aphiria/net": "1.0.*@dev",
        "ext-json": "*",
        "opulence/authentication": "dev-master",
        "opulence/authorization": "dev-master",
        "opulence/cryptography": "dev-master",
        "opulence/databases": "dev-master",
        "opulence/validation": "dev-master",
        "opulence/views": "dev-master",
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.14",
        "phpunit/phpunit": "~8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
